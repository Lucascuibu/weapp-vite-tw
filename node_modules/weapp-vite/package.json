{
  "name": "weapp-vite",
  "type": "module",
  "version": "1.1.1",
  "description": "weapp-vite 一个现代化的小程序打包工具",
  "author": "SonOfMagic <qq1324318532@gmail.com>",
  "license": "MIT",
  "homepage": "https://vite.icebreaker.top/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonofmagic/weapp-tailwindcss.git",
    "directory": "packages/weapp-vite"
  },
  "bugs": {
    "url": "https://github.com/sonofmagic/weapp-tailwindcss/issues"
  },
  "keywords": [
    "weapp",
    "vite",
    "ts",
    "wxml",
    "wxss",
    "mp",
    "miniprogram",
    "scss"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs",
      "require": "./dist/config.cjs"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "weapp-vite": "bin/weapp-vite.js",
    "weapp-vite-esm": "bin/weapp-vite-esm.js",
    "weapp-vite-cjs": "bin/weapp-vite-cjs.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "peerDependencies": {
    "vite": ">=2.6.0"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "chokidar": "^3.6.0",
    "debug": "^4.3.6",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "htmlparser2": "^9.1.0",
    "klaw": "^4.1.0",
    "magic-string": "^0.30.11",
    "micromatch": "^4.0.8",
    "pathe": "^1.1.2",
    "picocolors": "^1.0.1",
    "pkg-types": "^1.2.0",
    "vite-tsconfig-paths": "^5.0.1",
    "@weapp-core/init": "^1.0.3",
    "@weapp-core/logger": "^1.0.1",
    "@weapp-core/shared": "^1.0.1",
    "weapp-ide-cli": "^2.0.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "dev": "tsup --watch --sourcemap",
    "build": "tsup",
    "release": "node scripts/release.js",
    "test:dev": "vitest",
    "test": "vitest run",
    "sync": "cnpm sync weapp-vite"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts"
}